    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Run ",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}\\run.js",
            "args": [
                "-b 192.168.169.255"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Run with audio",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}\\run_with_audio.js",
            "args": [
                "-b 192.168.169.255"
            ]
        }
    ]
}